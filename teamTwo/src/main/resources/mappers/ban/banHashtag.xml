<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Hashtag">

	
	<insert id="bymatchTag" parameterType="Hashtag">
	
		insert into hashtag
		
		(
		 chg_num, hashtag_num
		)
		
		values
		(
		 #{chg_num}, #{hashtag_num}
		)
	</insert>


</mapper>